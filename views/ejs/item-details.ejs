<%- include('./partials/head') %>
  <script src="/script.js" defer></script>
  </head>

  <body>
    <%- include('./partials/nav') %>
      <h1> Game #<%=game.id %>
      </h1>
      <div id="root">
        <div class="card-holder">
          <h3>
            <%= game.name %>
          </h3>
          <p>Platform:<a href="/games?attribute=platform&value=<%= game.platform %>">
              <%=game.platform %>
            </a></p>
          <p>Genre: <%=game.genre %>
          </p>
          <p>Publisher: <%=game.publisher %>
          </p>
          <p>Developer: <%=game.developer %>
          </p>
          <p>Rating: <%=game.rating %>
          </p>
        </div>
        <h2><a href="/games/all">Games List</a></h2>
      </div>
  </body>

  </html>